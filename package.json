{
  "name": "cypress-nordlayer-ui",
  "description": "Tests for Nordlayer using Cypress + Cucumber",
  "scripts": {
    "cypress:runner": "cypress open --e2e --browser chrome",
    "cypress:e2e:headless": "cypress run --spec cypress/e2e/features/* --headless --browser chrome",
    "cypress:e2e:headed": "cypress run --spec cypress/e2e/features/* --headed --browser chrome"

  },
  "keywords": [
    "cypress"
  ],
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^15.0.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.5",
    "cypress": "^12.0.2"
  }
}
